    set audioInput to 
    tell application System Preferences
        activate
        set current pane to pane com.apple.preference.sound
        tell its window Sound
            select row 2 of table 1 of scroll area 1 of tab group 1
            delay 0.5 -- Give the UI some time to switch
            set audioInput to (value of text field 1 of group 1 of tab group 1)
        end tell
        quit
    end tell
    return audioInput
    
